      \strng{namehash}{38ff65b1c9d6ab92cd9acfc24a5a6264}
      \strng{fullhash}{8255ce4012b355f8b01a9558f7ac4dca}
      \strng{bibnamehash}{38ff65b1c9d6ab92cd9acfc24a5a6264}
      \strng{authorbibnamehash}{38ff65b1c9d6ab92cd9acfc24a5a6264}
      \strng{authornamehash}{38ff65b1c9d6ab92cd9acfc24a5a6264}
