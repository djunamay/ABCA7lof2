        {{hash=90fe33afbc5131fb1f9f8e80ad7bd881}{%
           family={Girard},
           familyi={G\bibinitperiod},
           given={Victor},
           giveni={V\bibinitperiod}}}%
        {{hash=5070098f9eefaacfe130d0f0caf3b7be}{%
           family={Gould},
           familyi={G\bibinitperiod},
           given={Alex\bibnamedelima P},
