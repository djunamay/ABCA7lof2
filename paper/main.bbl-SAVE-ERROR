        {{hash=0e93e72cf01a18dd2791d52cf8bd9141}{%
           family={Holm},
           familyi={H\bibinitperiod},
           given={Marie-Louise},
           giveni={M\bibinithyphendelim L\bibinitperiod}}}%
        {{hash=42c90f772745593e2d8db85f63b32277}{%
           family={Kanekiyo},
           familyi={K\bibinitperiod},
           given={Takahisa},
           giveni={T\bibinitperiod}}}%
