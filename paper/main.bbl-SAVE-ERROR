      \strng{namehash}{7411a71f527c0595cbc2a0d3038ff8b0}
      \strng{fullhash}{7411a71f527c0595cbc2a0d3038ff8b0}
      \strng{bibnamehash}{7411a71f527c0595cbc2a0d3038ff8b0}
      \strng{authorbibnamehash}{7411a71f527c0595cbc2a0d3038ff8b0}
