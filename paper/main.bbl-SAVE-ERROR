      \strng{namehash}{e86a3d31fec0b3a46874bee58a011fae}
      \strng{fullhash}{a0862434bf315d91aeda58e9a6b5b068}
      \strng{bibnamehash}{e86a3d31fec0b3a46874bee58a011fae}
