      \strng{namehash}{37305b6d7283f3adbba7465dd5e47fdb}
      \strng{fullhash}{ec5296f42386cf69199cc2c81f37b79f}
      \strng{bibnamehash}{37305b6d7283f3adbba7465dd5e47fdb}
      \strng{authorbibnamehash}{37305b6d7283f3adbba7465dd5e47fdb}
