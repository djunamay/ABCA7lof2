      \strng{namehash}{8ac032b56a8436269d62b416a4ddb9e2}
      \strng{fullhash}{59360cc1da43dcfffd8a26c7e90a4006}
      \strng{bibnamehash}{8ac032b56a8436269d62b416a4ddb9e2}
