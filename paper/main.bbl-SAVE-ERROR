      \strng{namehash}{e6a2f9792a9efc3b3f7888a7cf57294d}
      \strng{fullhash}{77e76a5f49def9a56ca18c8a0cac54e6}
      \strng{bibnamehash}{e6a2f9792a9efc3b3f7888a7cf57294d}
      \strng{authorbibnamehash}{e6a2f9792a9efc3b3f7888a7cf57294d}
      \strng{authornamehash}{e6a2f9792a9efc3b3f7888a7cf57294d}
