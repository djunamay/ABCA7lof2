      \field{month}{12}
      \field{number}{52}
      \field{title}{Neuronal {ROS-induced} glial lipid droplet formation is altered by loss of Alzheimer's disease--associated genes}
      \field{volume}{118}
      \field{year}{2021}
      \field{pages}{e2112095118}
      \range{pages}{-1}
