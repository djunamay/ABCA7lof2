      \strng{namehash}{33e67a96c0c11beffbfef0e37ac63594}
      \strng{fullhash}{52c7a55ad8aa104a4bf22031d0534b85}
      \strng{bibnamehash}{33e67a96c0c11beffbfef0e37ac63594}
      \strng{authorbibnamehash}{33e67a96c0c11beffbfef0e37ac63594}
      \strng{authornamehash}{33e67a96c0c11beffbfef0e37ac63594}
