      \strng{namehash}{66948fe6a471e058ab21f462606ce174}
      \strng{fullhash}{9359c1cbf89b5fc64cc4a9b418b9e9ed}
      \strng{bibnamehash}{66948fe6a471e058ab21f462606ce174}
      \strng{authorbibnamehash}{66948fe6a471e058ab21f462606ce174}
      \strng{authornamehash}{66948fe6a471e058ab21f462606ce174}
