      \strng{namehash}{fd9cdc707005df0dc1ab98198dee7c8d}
      \strng{fullhash}{fd9cdc707005df0dc1ab98198dee7c8d}
      \strng{bibnamehash}{fd9cdc707005df0dc1ab98198dee7c8d}
      \strng{authorbibnamehash}{fd9cdc707005df0dc1ab98198dee7c8d}
