      \strng{namehash}{54f3a7308c1ca9c89b621ce462dfda2d}
      \strng{fullhash}{2d32b8e9ad00a53df473d94f86de1e15}
      \strng{bibnamehash}{54f3a7308c1ca9c89b621ce462dfda2d}
      \strng{authorbibnamehash}{54f3a7308c1ca9c89b621ce462dfda2d}
      \strng{authornamehash}{54f3a7308c1ca9c89b621ce462dfda2d}
