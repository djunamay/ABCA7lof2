      \strng{namehash}{47dca8a0ea8acb663e65687a48e753df}
      \strng{fullhash}{47dca8a0ea8acb663e65687a48e753df}
      \strng{bibnamehash}{47dca8a0ea8acb663e65687a48e753df}
      \strng{authorbibnamehash}{47dca8a0ea8acb663e65687a48e753df}
