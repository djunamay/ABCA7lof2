      \strng{namehash}{5ab394e7e6df9978c1bdf38a81bbc23a}
      \strng{fullhash}{41ee40e845119bb187ca48a502c8fbf0}
      \strng{bibnamehash}{5ab394e7e6df9978c1bdf38a81bbc23a}
      \strng{authorbibnamehash}{5ab394e7e6df9978c1bdf38a81bbc23a}
