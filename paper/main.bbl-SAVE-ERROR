    \entry{Inazu2008-wg}{article}{}
      \name{author}{2}{}{%
        {{hash=ac1f7113e77df58d625b7c5c19318560}{%
           family={Inazu},
           familyi={I\bibinitperiod},
           given={Masato},
           giveni={M\bibinitperiod}}}%
        {{hash=ca4fe95fc5706871a61b40d9d097efbc}{%
           family={Matsumiya},
           familyi={M\bibinitperiod},
           given={Teruhiko},
           giveni={T\bibinitperiod}}}%
