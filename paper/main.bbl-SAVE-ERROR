      \strng{namehash}{8e27f3dde1c2f9d783d2cdcf3517aa68}
      \strng{fullhash}{c0d0c36702360ea91a0ee3e0e6bd098d}
      \strng{bibnamehash}{8e27f3dde1c2f9d783d2cdcf3517aa68}
