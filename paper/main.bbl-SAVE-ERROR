      \strng{namehash}{91b805dc11798dcd1c77b8e7a0f3e8e7}
      \strng{fullhash}{91b805dc11798dcd1c77b8e7a0f3e8e7}
      \strng{bibnamehash}{91b805dc11798dcd1c77b8e7a0f3e8e7}
      \strng{authorbibnamehash}{91b805dc11798dcd1c77b8e7a0f3e8e7}
      \strng{authornamehash}{91b805dc11798dcd1c77b8e7a0f3e8e7}
